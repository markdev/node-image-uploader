extends ./judgeLayout.jade

block title
	title Judge a specific contest

block content
	div#judgingWrapper
		h1 #{contest.title}
		p#position stuff
		div#carousel
			table
				tr		
					each entry, i in entries
						td(id="#{entry.id}")
							div.imageFrame
								img(src="/entries/#{entry.picture}")
		script(type='text/javascript').
			$('document').ready(function() {
				judge();
			});


	
